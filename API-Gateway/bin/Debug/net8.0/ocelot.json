{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/auth/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "auth-service",
                    "Port": 5001
                }
            ],
            "UpstreamPathTemplate": "/api/data/{everything}",
            "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "DownstreamPathTemplate": "/api/data/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "postgres",
                    "Port": 5002
                }
            ],
            "UpstreamPathTemplate": "/data/{everything}",
            "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"]
        },
        {
            "DownstreamPathTemplate": "/api/notifications/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
                {
                    "Host": "notification-service",
                    "Port": 5003
                }
            ],
            "UpstreamPathTemplate": "/notifications/{everything}",
            "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"]
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:5000"
    }
}